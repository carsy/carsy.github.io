{
    "root": true,
    "extends": [
        "@carsy/eslint-config/es6",
        "@carsy/eslint-config/addons/es6-modules",
        "@carsy/eslint-config/addons/browser",
        "@carsy/eslint-config/addons/react"
    ],
    "parserOptions":{
        "ecmaVersion": 9,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "react/no-multi-comp": 0
    },
    "globals": {
        "__dirname": false
    }
}
